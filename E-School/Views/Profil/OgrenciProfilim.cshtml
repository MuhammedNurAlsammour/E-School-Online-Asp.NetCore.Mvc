/*        strong {
            font-weight: 500;
            font-size: 19px;
            color: #000;
        }

        .mb-0 {
            font-size: 19px;
        }*/

        .profile-pic {
            color: transparent;
            transition: all 0.3s ease;
            display: flex;
            justify-content: center;
            align-items: center;
            position: relative;
            transition: all 0.3s ease;
        }

            .profile-pic input {
                display: none;
            }

            .profile-pic img {
                position: absolute;
                object-fit: cover;
                width: 165px;
                height: 165px;
                box-shadow: 0 0 10px 0 rgba(255, 255, 255, .35);
                border-radius: 100px;
                z-index: 0;
            }

            .profile-pic .-label {
                cursor: pointer;
                height: 165px;
                width: 165px;
            }

            .profile-pic:hover .-label {
                display: flex;
                justify-content: center;
                align-items: center;
                background-color: rgba(0, 0, 0, .8);
                z-index: 10000;
                color: #fafafa;
                transition: background-color 0.2s ease-in-out;
                border-radius: 100px;
                margin-bottom: 0;
            }

            .profile-pic span {
                display: inline-flex;
                padding: 0.2em;
                height: 2em;
            }

        .card-footer {
            border-color: #DBDBDB;
            background: transparent;
            padding: 1.25rem 1rem 2rem;
        }
    </style>

    <style>
        .my-table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

            .my-table td, .my-table th {
                border: 1px solid #dddddd;
                text-align: center;
                padding: 0px;
            }

            .my-table tr:nth-child(even) {
                background-color: #dddddd;
            }

            .my-table  tbody tr td {
                vertical-align: middle;
                border-color: #DBDBDB;
                white-space: nowrap;
            }

            .my-table  th, .my-table td {
                border-color: #DBDBDB;
                padding: 0px;
            }

            .my-table .card-table th:last-child, .my-table .card-table td:last-child {
                padding-right: 0px;
            }

            .my-table .card-body {
                padding: 1rem 1rem 1rem 0rem;
            }

            .my-table .card-table th:first-child, .my-table .card-table td:first-child {
                padding-left: 0rem;
            }
        .thh {
			text-align: center;
		}
    </style>

    <link href="~/Content/css/respom.css" rel="stylesheet" asp-append-version="true">

}

