<div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-hidden="true" id="DuzogrenciModal">
    <div class="modal-dialog modal-dialogs">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Düzenleme Penceresi</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal">
                </button>
            </div>
            <div class="modal-body">
                <div class="basic-form">
                    <form action="#" id="DuzogrenciForm" method="post">
                        @Html.AntiForgeryToken()

                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Adı</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" name="Adi" required>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Soyadı</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" name="Soyadi" required>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Email Adresi</label>
                            <div class="col-sm-9">
                                <input type="email" class="form-control" name="Email" required>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Telefon</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" name="Telefon" maxlength="10" onkeypress="validate(event)" required>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Doğum Tarihi</label>
                            <div class="col-sm-9">
                                <input type="date" class="form-control" name="DogumTarihi" required>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">TC Kimlik</label>
                            <div class="col-sm-9">
                                <input type="text" maxlength="11" onkeypress="validate(event)" class="form-control" name="IdentityNumber" required>
                            </div>
                        </div>
                        <fieldset class="mb-3">
                            <div class="row">
                                <label class="col-form-label col-sm-3 pt-0" style="font-size:12.25px; line-height: 2.4rem;">Kurumsal/Bireysel</label>
                                <div class="col-sm-9">
                                    <input class="form-check-input" type="radio" name="KurumsalBireysel" value="Kurumsal">
                                    <label class="form-check-label">
                                        Kurumsal
                                    </label>
                                    <input class="form-check-input" type="radio" name="KurumsalBireysel" value="Bireysel" checked>
                                    <label class="form-check-label">
                                        Bireysel
                                    </label>
                                </div>
                            </div>
                        </fieldset>
                        <fieldset class="mb-3">
                            <div class="row">
                                <label class="col-form-label col-sm-3 pt-0" style="font-size:12.25px; line-height: 2.4rem;">Cinsiyet</label>
                                <div class="col-sm-9">
                                    <input class="form-check-input" type="radio" name="Cinsiyet" value="Kadın" checked>
                                    <label class="form-check-label">
                                        Kadın
                                    </label>
                                    <input class="form-check-input" type="radio" name="Cinsiyet" value="Erkek">
                                    <label class="form-check-label">
                                        Erkek
                                    </label>
                                </div>
                            </div>
                        </fieldset>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Adres</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" name="Adres" required>
                            </div>
                        </div>
                        <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Açıklama</label>
                            <div class="col-sm-9">
                                <input type="text" class="form-control" name="Aciklama" required>
                            </div>
                        </div>
                        <fieldset class="mb-3">
                            <div class="row">
                                <label class="col-form-label col-sm-3 pt-0" style="font-size:12.25px; line-height: 2.4rem;">Veli Durumu</label>
                                <div class="col-sm-9">
                                    <input class="form-check-input" type="radio" name="VeliVarMi" value="Var">
                                    <label class="form-check-label">
                                        Var
                                    </label>
                                    <input class="form-check-input" type="radio" name="VeliVarMi" value="Yok" checked>
                                    <label class="form-check-label">
                                        Yok
                                    </label>
                                </div>
                            </div>
                        </fieldset>
                        <div id="veliDurumu" hidden>
                            <div class="mb-3 row">
                                <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Veli Adı</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="VeliAdi">
                                </div>
                            </div>
                            <div class="mb-3 row">
                                <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Veli Soyadı</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="VeliSoyadi">
                                </div>
                            </div>
                            <div class="mb-3 row">
                                <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Veli Email Adresi</label>
                                <div class="col-sm-9">
                                    <input type="email" class="form-control" name="VeliEmail">
                                </div>
                            </div>
                            <div class="mb-3 row">
                                <label class="col-sm-3 col-form-label" style="font-size:12.25px; line-height: 2.4rem;">Veli Telefon</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="VeliTelefon" maxlength="10" onkeypress="validate(event)">
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" id="btnSubmit">Kaydet</button>

                <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Kapat</button>
            </div>
        </div>
    </div>
</div>

